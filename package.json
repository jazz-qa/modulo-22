{
  "name": "modulo22",
  "version": "1.0.0",
  "description": "Page objects and App actions",
  "main": "index.js",
  "scripts": {
    "test": "npm run cy:run",
    "start": "npx serverest",
    "cy:run": "npx cypress run",
    "cy:report": "npx mochawesome-merge ./mochawesome-report/mochawesome*.json  > mochawesome-report/report.json && npx marge mochawesome-report/report.json",
    "cy:run-ci": "start-server-and-test start http://localhost:3000/api-doc test",
    "test:allure": "npx cypress run --env allure=true && npm run allure:generate",
    "allure:open": "npx allure serve",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "sorry:cypress": "CYPRESS_API_URL=\"http://localhost:1234/\" npx cy2 run --record --key EBAC --parallel --ci-build-id `date +%s`"
  },
  "author": "Jessica Oliveira",
  "license": "ISC",
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.13.8",
    "cypress": "^14.2.0",
    "cypress-html-reporter": "^11.0.0",
    "faker-br": "^0.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "cypress-mochawesome-reporter": "^4.0.0",
    "start-server-and-test": "^2.0.12"
  }
}
